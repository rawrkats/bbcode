{
    "name": "rawrkats/bbcode",
    "description": "BBCode parser from or to HTML.",
    "keywords": ["bbcode", "parser", "laravel", "psr-1", "psr-2", "psr-4"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "rawrkats",
            "email": "vexiphne@gmail.com",
            "homepage": "https://github.com/rawrkats/bbcode"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rawrkats\\BBCode\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/rawrkats/bbcode/issues"
    },
    "minimum-stability": "stable",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "9.1.4"
    },
    "scripts": {
        "test": "phpunit --coverage-text"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawrkats\\BBCode\\BBCodeServiceProvider"
            ],
            "aliases": {
                "Invoice": "Rawrkats\\BBCode\\Facades\\BBCode"
            }
        }
    }
}
